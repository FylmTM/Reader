{
  "name": "app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "postbuild": "gzipper -t 1000 -i html,js,css ./build",
    "test": "react-scripts test",
    "fmt": "prettier --write \"src/**/*.{ts,tsx}\"",
    "fmt-check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "lint": "eslint --ext js,ts,tsx src && tsc",
    "lint-fix": "eslint --fix --ext js,ts,tsx src && tsc",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "wouter": "^2.4.0",
    "zustand": "^2.2.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.14",
    "@types/react": "^16.9.13",
    "@types/react-dom": "^16.9.4",
    "gzipper": "^3.4.1",
    "prettier": "1.19.1",
    "react-scripts": "3.3.0",
    "source-map-explorer": "^2.1.1",
    "typescript": "^3.7.2"
  },
  "proxy": "http://127.0.0.1:8080",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      "last 3 chrome versions",
      "last 3 chromeandroid versions",
      "last 1 safari versions",
      "last 1 ios versions",
      "last 2 firefox versions",
      "last 1 edge version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
